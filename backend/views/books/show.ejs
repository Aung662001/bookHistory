<h3><%= book.title %></h2>
<img
  src="<%= book.coverImagePath %>"
  alt="bookCoverImage"
  width="100"
  height="150"
/>
<div>Author: <%= book.author.name %></div>
<div>Published Date: <%= book.publishDate.toDateString() %></div>
<div>Page Count: <%= book.pageCount %></div>
<div>Description: <%= book.description %></div>
<a href="/books/<%= book.id%>/edit" class="btn btn-primary">Edit</a>
<%- include("../partials/delete_form.ejs",{url:`/books/${book.id}`}) %>
<a href="/authors/<%= book.author.id %>">View Author</a>
